<% layout('layout') -%>

<div class="container">

  <div class="row">
    <div class="col-sm-6 col-md-4">
      <img class="img-circle img-responsive img-center" src="<%= user.profile.picture %>" alt="profile pic" />
      <br>
      <p>Name: <%= user.profile.name %></p>
      <p>Email: <%= user.email %></p>
      <p>Address: <%= user.address %></p>
    </div>
  </div>
</div>
